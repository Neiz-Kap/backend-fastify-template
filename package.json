{
  "name": "fastify-typescript",
  "version": "1.0.1",
  "description": "",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node build/index.js | pino-pretty --colorize",
    "dev": "tsnd --no-notify --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts | pino-pretty --colorize",
    "watch": "tsc --watch",
    "prisma": "dotenv -e .env prisma",
    "db:gen": "prisma generate",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "style": "prettier src --check",
    "style:fix": "prettier src --write",
    "deadcode": "ts-prune | grep -v 'used in module' | sort",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,mts,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neiz-kap/backend-fastify-template.git"
  },
  "keywords": [
    "fastify",
    "typescript",
    "backend",
    "template",
    "swagger",
    "zod"
  ],
  "author": "Neiz-Kap",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neiz-kap/backend-fastify-template/issues"
  },
  "homepage": "https://github.com/neiz-kap/backend-fastify-template#readme",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.4.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "@prisma/client": "5.17.0",
    "aws-sdk": "^2.1116.0",
    "bcryptjs": "^2.4.3",
    "fastify": "^4.28.1",
    "fastify-file-upload": "^4.0.0",
    "fastify-type-provider-zod": "^2.0.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^7.10.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/http-errors": "1.8.2",
    "@types/jsonwebtoken": "8.5.8",
    "@types/lint-staged": "~13.3.0",
    "@types/node": "17.0.25",
    "@types/tap": "~15.0.11",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.8.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.8",
    "pino-pretty": "7.6.1",
    "prettier": "^3.3.3",
    "prisma": "5.17.0",
    "tap": "16.0.1",
    "ts-node-dev": "^1.1.8",
    "ts-prune": "^0.10.3",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "overrides": {
    "eslint": "^9.8.0"
  },
  "volta": {
    "node": "20.14.0"
  },
  "packageManager": "pnpm"
}