{
  "name": "fastify-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node build/index.js | pino-pretty --colorize",
    "dev": "ts-node-dev ./src/index.ts | pino-pretty --colorize",
    "postinstall": "npx typesync && husky install",
    "prisma": "dotenv -e .env prisma",
    "db:gen": "npm run prisma generate",
    "db:studio": "npm run prisma studio",
    "db:migrate": "npm run prisma migrate dev",
    "db:push": "npm run prisma db push",
    "lint": "./node_modules/.bin/eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hmake98/fastify-typescript.git"
  },
  "keywords": [],
  "author": "hmake98",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hmake98/fastify-typescript/issues"
  },
  "homepage": "https://github.com/hmake98/fastify-typescript#readme",
  "dependencies": {
    "@fastify/autoload": "^5.10.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.0",
    "@prisma/client": "^3.12.0",
    "aws-sdk": "^2.1116.0",
    "bcryptjs": "^2.4.3",
    "env-schema": "^4.0.0",
    "fastify": "^4.28.1",
    "fastify-file-upload": "^3.0.1",
    "fluent-json-schema": "^3.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "pino": "^7.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "8.4.1",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/http-errors": "1.8.2",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "17.0.25",
    "@types/pino": "7.0.4",
    "@types/prettier": "2.6.0",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "eslint": "8.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.8",
    "pino-pretty": "7.6.1",
    "prettier": "2.6.2",
    "prisma": "3.12.0",
    "tap": "16.0.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.6.3"
  },
  "lint-staged": {
    "*.{js,ts,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build"
    }
  }
}