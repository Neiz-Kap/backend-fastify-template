{
  "name": "fastify-typescript",
  "version": "1.0.1",
  "description": "",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node build/src/index.js | pino-pretty --colorize",
    "dev": "tsnd --no-notify --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts | pino-pretty --colorize",
    "watch": "tsc --watch",
    "prisma": "dotenv -e .env prisma",
    "db:gen": "prisma generate",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "style": "prettier src --check",
    "style:fix": "prettier src --write",
    "deadcode": "ts-prune | grep -v 'used in module' | sort",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,mts,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neiz-kap/backend-fastify-template.git"
  },
  "keywords": [
    "fastify",
    "typescript",
    "backend",
    "template",
    "swagger",
    "zod"
  ],
  "author": "Neiz-Kap",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neiz-kap/backend-fastify-template/issues"
  },
  "homepage": "https://github.com/neiz-kap/backend-fastify-template#readme",
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/env": "^5.0.1",
    "@fastify/formbody": "^8.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/swagger": "^9.2.0",
    "@fastify/swagger-ui": "^5.1.0",
    "@prisma/client": "5.20.0",
    "bcryptjs": "^2.4.3",
    "fastify": "^5.0.0",
    "fastify-file-upload": "^4.0.0",
    "fastify-type-provider-zod": "^4.0.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.5.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/http-errors": "2.0.4",
    "@types/jsonwebtoken": "9.0.7",
    "@types/lint-staged": "~13.3.0",
    "@types/node": "22.7.5",
    "@types/tap": "~15.0.12",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "pino-pretty": "11.2.2",
    "prettier": "^3.3.3",
    "prisma": "5.20.0",
    "tap": "21.0.1",
    "ts-node-dev": "^2.0.0",
    "ts-prune": "^0.10.3",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.9.0"
  },
  "overrides": {
    "eslint": "^9.12.0"
  },
  "volta": {
    "node": "20.14.0"
  },
  "packageManager": "pnpm"
}